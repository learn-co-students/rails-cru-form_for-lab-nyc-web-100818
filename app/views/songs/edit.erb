<h1>Edit Song</h1>
<%= form_for @song do |f| %>
  <%= f.label :name, 'Name' %><br>
  <%= f.text_field :name %><br>
  <%= f.label :artist_id, 'Artist' %><br>
  <%# f.select :artist_id, Artist.all.map { |a| [ a.name, a.id ] }, include_blank: true %>
  <%= f.number_field :artist_id %>
  <%= f.label :genre_id, 'Genre' %><br>
  <%# f.select :genre_id, Genre.all.map { |a| [ a.name, a.id ] }, include_blank: true %>
  <%= f.number_field :genre_id %>
  <%= f.submit "Update Song" %>
<% end %>